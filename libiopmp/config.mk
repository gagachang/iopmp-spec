# Set to 'y' to enable debug build
DEBUG=y

# Set to 'y' to enable compiling of register interface as weak functions
CFG_IOPMP_REF_MODEL=y

# Set to 'y' to enable compiling of driver for full model
CFG_IOPMP_DRV_FULL=y

# Set to 'y' to enable compiling of driver for rapid-k model
CFG_IOPMP_DRV_RAPID_K=y

# Set to 'y' to enable compiling of driver for dynamic-k model
CFG_IOPMP_DRV_DYNAMIC_K=y

# Set to 'y' to enable compiling of driver for isolation model
CFG_IOPMP_DRV_ISOLATION=y

# Set to 'y' to enable compiling of driver for compact-k model
CFG_IOPMP_DRV_COMPACT_K=y

# Set to 'y' to enable compiling of driver for SRCMD_FMT=1 & MDCFG_FMT=2
CFG_IOPMP_DRV_SRCMD_FMT_1_MDCFG_FMT_2=y

# Set to 'y' to enable compiling of driver for SRCMD_FMT=2 & MDCFG_FMT=0
CFG_IOPMP_DRV_SRCMD_FMT_2_MDCFG_FMT_0=y

# Set to 'y' to enable compiling of driver for SRCMD_FMT=2 & MDCFG_FMT=1
CFG_IOPMP_DRV_SRCMD_FMT_2_MDCFG_FMT_1=y

# Set to 'y' to enable compiling of driver for SRCMD_FMT=2 & MDCFG_FMT=2
CFG_IOPMP_DRV_SRCMD_FMT_2_MDCFG_FMT_2=y

# Set to 'y' to enable compiling of driver for SPS(Secondary Permission Setting)
CFG_IOPMP_DRV_SPS_EXTENSION=y
